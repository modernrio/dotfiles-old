! ------------------------------------------------------------------------------
! rofi color theme + keybindings
! ------------------------------------------------------------------------------
rofi.color-enabled:					true
!									Odd Rows	Text		Even Rows	Active row	Active Text
rofi.color-normal:					#273238,	#c1c1c1,	#273238,	#394249,	#ffffff
rofi.color-urgent:					#273238,	#ff1844,	#273238,	#394249,	#ff1844
rofi.color-active:					#273238,	#80cbc4,	#273238,	#394249,	#80cbc4
!									Background	Border
rofi.color-window:					#273238,	#273238,	#1e2529
rofi.separator-style:               dash

rofi.kb-remove-to-eol:              
rofi.kb-accept-entry:               Control+m,Return,KP_Enter
rofi.kb-row-up:                     Control+k,Up,Control+p,Shift+Tab,Shift+ISO_Left_Tab
rofi.kb-row-down:                   Control+j,Down,Control+n
